<% provide(:title, 'All users') %>
<%= form_tag users_path, :method => 'get' do %> <%#　projects_pathはモデル名で読みかえること %>
<p>
  <%= text_field_tag :search, params[:search] %>
  <%= submit_tag "Search", :name => nil %>
</p>
<% end %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
  <%= render @users %>
</ul>